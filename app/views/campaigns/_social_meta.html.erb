<meta name="twitter:card" content="summary">
<meta name="twitter:site" content="@phunderapp">
<meta name="twitter:title" content="<%= @campaign.name %>">
<meta name="twitter:description" content="<%= @campaign.description %>">
<meta name="twitter:creator" content="@punderapp">
<meta name="twitter:image:src" content="<%= @campaign.image.url(:square) %>">
<meta name="twitter:domain" content="phunderapp.com">
<meta property="og:title" content="<%= @campaign.name %>" />
<meta property="og:url" content="http://phunderapp.com/campaigns/<%= @campaign.id %>" />
<meta property="og:image" content="<%= @campaign.image.url(:square) %>" />